<Application x:Class="BookRegister.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vml="clr-namespace:BookRegister.ViewModels.Locator"
             xmlns:vc="clr-namespace:BookRegister.ValueConverters"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <vml:ViewModelLocator x:Key="ViewModelLocator" />
        <vc:DateValueConverter x:Key="DateValueConverter" />
        <vc:BusyToVisibilityConverter x:Key="BusyToVisibilityConverter" />
    </Application.Resources>
</Application>
